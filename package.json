{
  "name": "customauth",
  "version": "0.1.0",
  "bin": {
    "customauth": "bin/customauth.js"
  },
  "scripts": {
    "build": "tsc",
    "build:watch": "tsc -w",
    "clearbuildcache": "rm -rf ./build && rm -rf ./bundles",
    "test": "jest",
    "test:watch": "jest --watch",
    "cdk": "cdk",
    "cdk:bootstrap": "cdk bootstrap -y -o templates",
    "cdk:synth": "cdk synth -y -o templates",
    "cdk:diff": "cdk diff -y -o templates",
    "cdk:deploy": "cdk deploy -y -o templates --all",
    "cdk:destroy": "cdk destroy -y -o templates --all"
  },
  "devDependencies": {
    "@aws-cdk/assert": "1.87.0",
    "@types/jest": "^26.0.10",
    "@types/node": "10.17.27",
    "aws-cdk": "1.87.0",
    "jest": "^26.4.2",
    "ts-jest": "^26.2.0",
    "ts-node": "^9.0.0",
    "typescript": "~3.9.7"
  },
  "dependencies": {
    "@aws-cdk/aws-apigateway": "^1.91.0",
    "@aws-cdk/aws-dynamodb": "^1.91.0",
    "@aws-cdk/aws-lambda": "^1.91.0",
    "@aws-cdk/core": "^1.91.0",
    "@types/aws-lambda": "^8.10.72",
    "dotenv": "^8.2.0",
    "source-map-support": "^0.5.16"
  }
}
